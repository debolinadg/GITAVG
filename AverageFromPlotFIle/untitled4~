function createfigure(X1, YMatrix1, YData1, XData1, X2, Y1, X3, Y2)
%CREATEFIGURE(X1, YMATRIX1, YDATA1, XDATA1, X2, Y1, X3, Y2)
%  X1:  vector of x data
%  YMATRIX1:  matrix of y data
%  YDATA1:  patch ydata
%  XDATA1:  patch xdata
%  X2:  vector of x data
%  Y1:  vector of y data
%  X3:  vector of x data
%  Y2:  vector of y data

%  Auto-generated by MATLAB on 08-Aug-2018 13:45:15

% Create figure
figure1 = figure;

% Create axes
axes1 = axes('Parent',figure1);
hold(axes1,'on');

% Create multiple lines using matrix input to plot
plot1 = plot(X1,YMatrix1,'LineWidth',0.005,'LineStyle','none',...
    'Color',[0 0 0],...
    'Parent',axes1);
set(plot1(3),'LineWidth',3,'LineStyle','-');

% Create patch
patch('Parent',axes1,'YData',YData1,'XData',XData1,'LineStyle','none',...
    'FaceColor',[0.800000011920929 0.800000011920929 0.800000011920929]);

% Create plot
plot(X2,Y1,'LineWidth',3,'LineStyle','--','Color',[0 0 0]);

% Create plot
plot(X3,Y2,'LineWidth',3,'LineStyle',':','Color',[0 0 0]);

% Create xlabel
xlabel('$Temperature(K)$','Interpreter','latex');

% Create ylabel
ylabel('$RR(1/m^3-s)$','Interpreter','latex');

% Create title
title('$CO+OH{\rightarrow}CO_2+H$      ','Interpreter','latex');

box(axes1,'on');
grid(axes1,'on');
% Set the remaining axes properties
set(axes1,'FontSize',24);
% Create textbox
annotation(figure1,'textbox',...
    [0.167504381694255 0.792452830188679 0.119740019474197 0.0754716981132075],...
    'String',{'$Ka=108$'},...
    'Interpreter','latex',...
    'FontSize',18,...
    'FitBoxToText','off',...
    'EdgeColor',[1 1 1]);

